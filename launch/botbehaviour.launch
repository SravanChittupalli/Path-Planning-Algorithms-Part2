    <param name = "dest_pos_y" value="$(arg dest_y)"/>

    <node pkg="bug1and2_algorithm" type="wall_follow.py" name="wall_follow" output="screen"/>
    <node pkg="bug1and2_algorithm" type="go_to_pos.py" name="go_to_pos" output="screen"/>
    
</launch>
